{
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.tabSize": 2,
    "extensions.ignoreRecommendations": true,
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "search.useIgnoreFiles": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "workbench.productIconTheme": "fluent-icons",
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Ayu Dark (Legacy)",
    "workbench.tree.enableStickyScroll": false,
    "workbench.editorLargeFileConfirmation": 100,
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "remote.SSH.useExecServer": false,
    "systemverilog.formatCommand": "verible-verilog-format --column_limit 250 --assignment_statement_alignment infer --case_items_alignment align --module_net_variable_alignment align --named_parameter_alignment align --named_port_alignment align --named_port_indentation wrap --port_declarations_alignment infer",
    "systemverilog.excludeIndexing": "**/*.v",
    "editor.detectIndentation": false,
    "files.autoSave": "afterDelay",
    "editor.folding": false,
    "editor.foldingHighlight": false,
    "editor.glyphMargin": false,
    "systemverilog.documentSymbolsPrecision": "full_no_references",
    "systemverilog.includeIndexing": [
      "**/*.{sv,svh}"
    ],
    "systemverilog.disableIndexing": true,
    "systemverilog.enableIncrementalIndexing": false,
    "search.exclude": {        
      "mapped/**/*": true,
      "work/dcnx*/**/*":true
    },
    "files.associations": {
      "*.do": "tcl",
      "*.log": "plaintext"
    },
    "workbench.iconTheme": "vs-nomo-dark",
    "editor.stickyScroll.scrollWithEditor": false,
    "workbench.editorAssociations": {
      "*.bin": "default"
    },
    "folderformatter.extensions": [

      "sv",
      "v",
      "svh",
      "vh"
    ],
    "diffEditor.maxComputationTime": 0,
    "hediet.vscode-drawio.resizeImages": null,
    "files.simpleDialog.enable": true,
    "security.workspace.trust.untrustedFiles": "open",
    "window.commandCenter": false,
    "window.customTitleBarVisibility": "never",
    "window.titleBarStyle": "custom",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "git.openRepositoryInParentFolders": "never",
    "chat.commandCenter.enabled": false,
    "editor.minimap.enabled": false,
    "remote.SSH.externalSSH_ASKPASS": true
}